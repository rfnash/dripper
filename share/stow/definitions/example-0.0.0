#!/usr/bin/env bash

PACKAGE=example
VERSION=0.0.0
DEBUG=true

echo "Package: $PACKAGE"
echo "Version: $VERSION"

set_variables

if [ ! -z $_INSTALL ]; then
  echo "=> Install"
  exit
fi

if [ ! -z $_UNINSTALL ]; then
  echo "=> Uninstall"
  exit
fi

if [ ! -z $_INFO ]; then
  echo "=> Info"
  exit
fi
